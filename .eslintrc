{
  "rules": {
    "indent": [
      2,
      2,
      {"VariableDeclarator": { "var": 2, "let": 2, "const": 3}}
    ],
    "linebreak-style": [2, "unix"],
    "semi": [2, "always"],
    "comma-dangle": [2, "always-multiline"],
    "no-console": 1,
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "modules": true,
    "experimentalObjectRestSpread": true
  },
  "globals": {
    "require": false,
    "module": true,
    "Environment": false,
    "__dirname": false,
    "process": false
  }
}

